{
  "name": "fastify-lambda",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pm2 start process.json",
    "lambda": "npm run lambda:deploy",
    "lambda:deploy": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1",
    "lambda:update": "claudia update",
    "lambda:pack": "claudia pack",
    "lint": "semistandard | snazzy",
    "start": "NODE_ENV=dev nodemon -L -w ./src ./src/server.js",
    "test": "npm run lint && npm run test:unit",
    "test:coverage": "npm run test:unit -- --cov --coverage-report=text --coverage-report=html",
    "test:load": "artillery run ./test-load/root.v1.yml",
    "test:unit": "tap ./test/**/*.test.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^2.2.1",
    "aws-lambda-fastify": "^1.3.0",
    "compression": "^1.7.4",
    "dotenv": "^8.1.0",
    "fastify": "^2.8.0",
    "fastify-autoload": "^1.0.0",
    "fastify-cors": "^2.1.3",
    "fastify-helmet": "^3.0.1",
    "fastify-jwt": "^1.1.0",
    "fastify-plugin": "^1.6.0",
    "fastify-static": "^2.5.0",
    "fs-extra": "^8.1.0",
    "path": "^0.12.7",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "artillery": "^1.6.0-29",
    "cheerio": "^1.0.0-rc.3",
    "claudia": "^5.8.0",
    "fastify-cli": "^1.2.0",
    "nock": "^11.3.5",
    "nodemon": "^1.19.3",
    "pino": "^5.13.4",
    "pm2": "^3.5.1",
    "pre-commit": "^1.2.2",
    "semistandard": "^14.2.0",
    "snazzy": "^8.0.0",
    "tap": "^14.6.9"
  }
}
